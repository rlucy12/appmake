<WidgetsExample>:
    
    name: "Home"
  
    GridLayout:
        Image: 
            size: root.width, root.height
            pos:0,0
            source: "prep.png"
            allow_stretch: True
            keep_ratio: False
            
            
            Button:
                text: "Arithmetic"
                pos: 200 , 700
                color : .5,1,.5
                font_size: 70
                width: 450
                height:150
                background_color : 1,1,1
                on_release: app.root.current = "ssc"
        
            Button:
                text: "Reasoning"
                pos: 200 , 500
                color : .5,1,.5
                font_size: 70
                width: 450
                height:150
                background_color : 1,1,1
                on_release: app.root.current = "reasoning"
            Button:
                text: "English"
                pos: 200 , 300
                color : .5,1,.5
                font_size: 70
                width: 450
                height:150
                background_color : 1,1,1
                on_release: app.root.current = "english"
            Button:
                text: "Coding"
                pos: 200 , 100
                color : .5,1,.5
                font_size: 70
                height:150
                width: 450
                background_color : 1,1,1
                on_release: app.root.current = "code"
        

